package yada.model;

import java.util.Date;

import org.bson.types.ObjectId;
import org.mongodb.morphia.annotations.Id;

public class BaseEntity {
	@Id
	private ObjectId id;
	
	public String getId()
	{
		return id.toString();
	}
	private schemaVer,shardKey;
	private Date createdAt, lastModifiedOn, lastModifiedBy;

}
